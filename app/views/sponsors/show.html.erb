<p id="notice"><%= notice %></p>

<div class="card">
  <div class="card-body">
    <h2 class="card-title">Sponsor</h2>

    <p class="card-text">
      <strong>Title:</strong>
      <%= @sponsor.title %>
    </p>

    <p class="card-text">
      <strong>Website:</strong>
      <%= @sponsor.website %>
    </p>

    <%= link_to 'Edit', edit_sponsor_path(@sponsor), class: "btn btn-primary" %> |
    <%= link_to 'Back', sponsors_path, class: "btn btn-primary" %>
    
  </div>
</div>
